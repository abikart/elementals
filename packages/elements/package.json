{
	"name": "@elementals/elements",
	"version": "0.0.1",
	"description": "Make browser native HTML tags interactive and them-able with custom elements.",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"dev": "pnpm tsup --watch",
		"build": "tsup"
	},
	"exports": {
		"./button": {
			"types": "./dist/elements/button/index.d.ts",
			"default": "./dist/elements/button/index.js"
		},
		"./accordion": {
			"types": "./dist/elements/accordion/index.d.ts",
			"default": "./dist/elements/accordion/index.js"
		},
		"./color/primitives": {
			"types": "./dist/tokens/primitives.d.ts",
			"default": "./dist/tokens/primitives.js"
		},
		"./color": {
			"types": "./dist/tokens/color.d.ts",
			"default": "./dist/tokens/color.js"
		},
		"./dist/*": "./dist/*",
		"./color.css": "./dist/color.css"
	},
	"files": ["dist"],
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^22.7.5",
		"colorjs.io": "^0.5.2",
		"tsup": "^8.3.0",
		"typescript": "^5.6.3"
	}
}
